{
  "name": "@testing-altsc/server",
  "version": "0.1.0",
  "description": "Server",
  "main": "index.js",
  "scripts": {
    "dev": "npm run nodemon",
    "start": "export APP_ENV=production; node -r ts-node/register/transpile-only -r dotenv/config build/index.js",
    "build": "tsc",
    "nodemon": "nodemon  --watch src -e ts --exec \"ts-node -r ts-node/register/transpile-only -r dotenv/config src/index.ts\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.5",
    "@types/glob": "^7.1.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/passport-anonymous": "^1.0.3",
    "@types/passport-jwt": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "concurrently": "^6.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.10",
    "mongoose-private": "^0.1.3",
    "mongoose-unique-validator": "^2.0.3",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.0"
  }
}
